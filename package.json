{
  "name": "plp",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "test": "jest",
    "test:watch": "yarn test --watch",
    "lint": "eslint --ext js,jsx,ts,tsx .",
    "lint:fix": "yarn lint --fix",
